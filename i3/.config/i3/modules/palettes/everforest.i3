# Everforest Theme

set $bg0 #232A2E
set $bg1 #2D353B
set $bg2 #475258
set $bg3 #4F585E
set $bg4 #56635f

set $fg0 #BBC3D4
set $fg1 #D3C6AA

set $ignore #000000
set $highlight #E69875

# class                 border      bground      text 	indicator child_border
client.focused          $bg2        $bg1         $fg1 	$bg3      $bg2
client.focused_inactive $bg1        $bg0         $fg0 	$bg1      $bg1 
client.unfocused        $bg1        $bg0         $bg4 	$bg1      $bg1 
client.urgent           $bg1        $highlight   $bg1 	$bg1      $bg1 
client.placeholder      $ignore     $bg1         $fg0 	$ignore   $bg2
client.background       $bg1 



